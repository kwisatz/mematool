#
# syn2cat custom LDAP schema
#

# PGP key
attributetype ( 1.3.6.1.4.1.37126.1.1.2.10 NAME 'pgpKey'
		DESC 'PGP key full fingerprint'
		EQUALITY caseIgnoreMatch
		SYNTAX 1.3.6.1.4.1.1466.115.121.1.44{40})

# Convention signed by
attributetype ( 1.3.6.1.4.1.37126.1.1.2.11 NAME 'conventionSigner'
		DESC 'UID of the member signing the member social contract'
		EQUALITY caseIgnoreMatch
		SYNTAX 1.3.6.1.4.1.1466.115.121.1.44{64})

attributetype ( 1.3.6.1.4.1.37126.1.1.2.12 NAME 'xmppID'
                EQUALITY caseIgnoreIA5Match
                SUBSTR caseIgnoreIA5SubstringsMatch
                SYNTAX 1.3.6.1.4.1.1466.115.121.1.26{256} )
		DESC 'XMPP ID')

attributetype ( 1.3.6.1.4.1.37126.1.1.2.13 NAME 'iButtonUID'
		DESC 'iButton UID'
		EQUALITY caseIgnoreMatch
		SYNTAX 1.3.6.1.4.1.1466.115.121.1.44{10})

attributetype ( 1.3.6.1.4.1.37126.1.1.2.14 NAME 'spaceKey'
		DESC 'spaceKey'
		EQUALITY booleanMatch
		SYNTAX 1.3.6.1.4.1.1466.115.121.1.7 )

attributetype ( 1.3.6.1.4.1.37126.1.1.2.15 NAME 'npoMember'
		DESC 'npoMember'
		EQUALITY booleanMatch
		SYNTAX 1.3.6.1.4.1.1466.115.121.1.7 )

attributetype ( 1.3.6.1.4.1.37126.1.1.2.16 NAME 'nationality'
		DESC 'Nationality'
		EQUALITY caseIgnoreMatch
		SYNTAX 1.3.6.1.4.1.1466.115.121.1.44{2})

objectClass ( 1.3.6.1.4.1.37126.1.1.2.1 NAME 'syn2catPerson'
		DESC 'syn2cat Person'
		SUP top AUXILIARY
		MAY ( pgpKey $ conventionSigner $ xmppID $ iButtonUID $ spaceKey $ npoMember $ nationality ) )
